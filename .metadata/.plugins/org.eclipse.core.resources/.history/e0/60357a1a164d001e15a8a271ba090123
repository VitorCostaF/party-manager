package br.com.ieoafestasedecoracoes.partymanager.clientservice.domain;

import jakarta.persistence.Entity;
import jakarta.persistence.Id;
import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor
@Entity
public class Client {

	@Id
	private Integer id;
	private String cpf;
	private String firstName;
	private String lastName;
	
	
}
